package dev.jason.primitives;

public class StudentPlayground {

	public static void main(String[] args) {
		Student jaxon = new Student();
		Student williams = new Student();
		
		jaxon.name = "Jaxon Smith";
		jaxon.age = 22;
		
		williams.name = "John Williams";
		williams.age = 54;
		
		System.out.println(jaxon.name + " is " + jaxon.age + " years old.");
		System.out.println(williams.name + " is " + williams.age + " years old.");
	}
}
