package dev.jason.working;

public class Cat {

	// Data Memebers
	private String name;
	private int age;
	
	// Getters and setters
	public void setAge(int age) {
		if (age < 0) 		return;
		else 				this.age = age;
	}
	public int getAge() {
		return this.age;
	}
	
	
	public void printInfo() {
		System.out.println("The cats age is " + this.age);
		this.age = -70;
	}

}
